package com.example.demo;

import org.springframework.stereotype.Component;

@Component

public class DaoIml implements demodao {
	public  DaoIml()
	{
		System.out.print("dao iml:: constructor");
	}
	
	@Override	
	public boolean findNameById(int i)
	{
	if(i==100)
	{
		return "Subbu";
	}
	else if(i==200)
	{
		return "Hare";
		
	}
	else
		return "Krishna";
	}
	

}
